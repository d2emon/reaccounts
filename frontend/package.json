{
  "name": "reaccounts-frontend",
  "version": "0.1.0",
  "description": "Frontend for reaccounts",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --inline --hot --progress --mode development",
    "start:react": "react-scripts start",
    "build:dev": "NODE_ENV=development webpack -w --progress --mode development",
    "build:dev:server": "NODE_ENV=development webpack --progress && babel-node server.js",
    "build:prod:server": "NODE_ENV=production webpack --progress && babel-node server.js",
    "build:react": "react-scripts build",
    "test:react": "react-scripts test --env=jsdom",
    "eject:react": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/d2emon/reaccounts.git"
  },
  "keywords": [
    "accounts"
  ],
  "author": "Dmitry Kutsenko <d2emonium@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/d2emon/reaccounts/issues"
  },
  "homepage": "https://github.com/d2emon/reaccounts#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "svg-loader": "0.0.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-config": "^7.5.0",
    "webpack-dev-server": "^3.1.4"
  }
}
